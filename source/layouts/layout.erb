<!doctype html>
<html>
    <head>
        <%= partial 'partials/layout-head' %>
    </head>

    <body ontouchstart="" class="<%= page_classes %>">
        <%= partial 'partials/title' %>
        <% if current_page.path == 'index.html' %>
            <%= partial 'partials/intro', :locals => { :offset => true } %>
        <% end %>
        <%= partial 'partials/nav' %>
        <%= yield %>
        <%= partial 'partials/footer' %>
    </body>
</html>
